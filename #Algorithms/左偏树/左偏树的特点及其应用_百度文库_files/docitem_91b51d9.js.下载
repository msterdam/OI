define("wkview:widget/kg_recommend/component/doclist/docitem/docitem.js",function(t,i){var e=t("wkview:widget/ui/html_view/widget.js"),o=e.widget,s=t("wkview:widget/kg_recommend/view/statistic.js").Statistic,a=t("wkcommon:widget/ui/lib/jquery/jquery.js"),n=t("wkcommon:widget/lib/doT/doT.min.js"),c=(t("wkcommon:widget/ui/js_core/log/log.js"),n.template('<div class="item clearfix">\n<div class="title-wrap">\n<b class="ic ic-{{=it.type}}"></b>\n<a class="title log-xsend" data-icon="{{=it.icon}}" data-logxsend="[1,101200, {\'icon\':\'{{=it.icon}}\'}]" href="{{=it.uri}}" title=\'{{=it.title}}\' target="_blank">{{=it.title}}</a>\n</div>\n<span class="price">{{=it.pay}}</span>\n<span class="score" title=\'{{=it.score}}\u5206\'>{{=it.scoreStr || \'\'}}</span>\n</div>\n')),r=n.template('{{? it.score }}<span>\n{{ for(var i=0; i< it.onNum; i++) { }}\n        <b class="ic ic-star-s-on"></b>\n{{ } }}        {{? it.halfNum == 1 }}<b class="ic ic-star-s-half"></b>\n{{?}}        {{ for(var j=0; j< it.offNum; j++) { }}\n        <b class="ic ic-star-s-off"></b>\n{{?}}</span>\n{{??}}    \u6682\u65e0\u8bc4\u4ef7{{?}}'),d={word:"doc",txt:"txt",ppt:"ppt",pdf:"pdf",excel:"xls",video:"video"},l=o({Options:{},elements:{".score":"$scoreWrap",".title-wrap":"$titleWrap"},_init:function(){this.mediator=this.options.mediator,this.data=this.options.data},_initEvents:function(){var t=this;t.on("onload",function(){}),t.on("ondispose",function(){})},_render:function(){var t=this,i=t.data;i.type=d[i.type]||"doc",i.scoreStr=t._createStarRate(),a.isNumeric(i.score)||(i.score="0"),t.$el.append(c(i))},_createStarRate:function(){var t=this,i="",e=t.data.score,o=0;if(e&&a.isNumeric(e)){var s=Math.floor(e),n=5-Math.ceil(e);e>s&&(o=1),i=r({score:e,onNum:s,offNum:n,halfNum:o})}else i="\u6682\u65e0\u8bc4\u5206";return i},calTitleMaxWidth:function(t){var i=t.kgdocsWrapW,e=this.$elements.$titleWrap;e&&e.width(i-167)}},{type:"docitem"});e.register(l,s,s.prototype),i.Docitem=l});