define("wkview:widget/ui/html_view/slider/pageNum.js",function(t,i,e){var n=t("wkview:widget/ui/html_view/dep/string/string.js"),l=t("wkview:widget/ui/html_view/widget.js").widget,a=(t("wkview:widget/ui/html_view/slider/control.js").Control,l({_init:function(){var t=this;t.tmpl='<div class="ipt-box">\n    <label><input type="text" value="1" class="current-page" js-hook="toolbar-ipt" style="width: 42px;padding-right: 2px;margin-right: 2px;text-align: right"></label>\n    <span class="total-page">/&nbsp;#{totalPage}</span>\n</div>',t.tmpl=n.format(t.tmpl,{totalPage:t.options.countPn})},_initEvents:function(){},_render:function(){$(".centerLeft").append(this.tmpl)}}));e.exports.PageNum=a});