define("wkview:widget/kg_recommend/kg_recommend.js",function(e,t){function i(e){var t=new o({el:e.el,docWrap:e.docWrap,Mediator:n});t.on("onload",function(){d(n,{doc_id:e.docId}),c(n,{doc_id:e.docId}),r(n,{doc_id:e.docId}),n.fire("v:initFirstKGInfo",e.data||{}),n.on("s:getDataByTabItem",function(t){var i=t&&t.ent_uuid;n.fire("s:getCardDocsByEntid",{type:s.type.getCardDocsByEntid,doc_id:e.docId,entUuid:i}).then(function(e){n.fire("v:refreshKGTreeDoclist",e)})}),n.on("s:getDoclistBySwitch",function(e){var t=e.entUuid,i=e.pn;n.fire("s:getDoclistByPn",{type:s.type.getDoclistByPn,pn:i,entUuid:t}).then(function(e){n.fire("v:refreshDoclist",e)})}),n.on("s:getDoclistByChangeTreeNode",function(e){var t=e.entUuid,i=e.entName,o={ent_uuid:t,ent_name:i},d=e.pn;n.fire("s:getDoclistByPn",{type:s.type.getDoclistByPn,pn:d,entUuid:t}).then(function(e){e.curNode=o,n.fire("v:refreshWholeDoclist",e)})})})}var n=e("wkview:widget/ui/pay/mediator.js").mediator,o=e("wkview:widget/kg_recommend/view/view.js").View,d=e("wkview:widget/kg_recommend/store/getContentData.js").getContentData,c=e("wkview:widget/kg_recommend/store/getCardDocsByEntid.js").getCardDocsByEntid,r=e("wkview:widget/kg_recommend/store/getDoclistByPn.js").getDoclistByPn,s=e("wkview:widget/kg_recommend/util/config.js");t.main=i});